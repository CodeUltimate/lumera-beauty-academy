      "lastName": "Laurent",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "Educator123!",
          "temporary": false
        }
      ],
      "realmRoles": [
        "EDUCATOR"
      ],
      "attributes": {
        "specialty": [
          "Aesthetics (Needle)"
        ],
        "bio": [
          "Board-certified dermatologist with 15+ years of experience"
        ],
        "educatorVerified": [
          "true"
        ]
      }
    },
    {
      "username": "student@lumera.com",
      "email": "student@lumera.com",
      "firstName": "Emma",
