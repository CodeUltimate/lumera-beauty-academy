        "composite": false
      },
      {
        "name": "ADMIN",
        "description": "Administrator role - full platform access",
        "composite": false
      }
    ],
    "client": {
      "lumera-backend": [],
      "lumera-frontend": []
    }
  },
  "defaultRoles": [
    "STUDENT"
  ],
  "requiredCredentials": [
    "password"
  ],
  "passwordPolicy": "length(8) and upperCase(1) and lowerCase(1) and digits(1)",
  "users": [
